{
  "name": "@poolifier/poolifier-web-worker",
  "version": "0.3.16",
  "exports": "./src/mod.ts",
  "compilerOptions": {
    "lib": ["deno.worker"],
    "strict": true
  },
  "tasks": {
    "benchmark:deno": "deno bench -A ./benchmarks/internal/bench.mjs",
    "benchmark:deno:json": "deno bench -A --json ./benchmarks/internal/bench.mjs",
    "benchmark:benchmark-js": "deno run -A --v8-flags=--max-old-space-size=8192 ./benchmarks/internal/bench.mjs -t benchmark.js ",
    "benchmark:benchmark-js:debug": "deno run -A --inspect --v8-flags=--max-old-space-size=8192 ./benchmarks/internal/bench.mjs -t benchmark.js ",
    "benchmark:tatami-ng": "deno run -A ./benchmarks/internal/bench.mjs -t tatami-ng",
    "benchmark:tatami-ng:debug": "deno run -A --inspect ./benchmarks/internal/bench.mjs -t tatami-ng",
    "bundle": "deno run -A bundle.ts",
    "test": "ENVIRONMENT=test deno test -A --parallel",
    "test:coverage": "ENVIRONMENT=test deno test -A --parallel --coverage=./coverage",
    "test:debug": "ENVIRONMENT=test deno test -A --inspect",
    "coverage": "deno coverage ./coverage",
    "coverage:report": "deno coverage ./coverage --lcov --output=./coverage/lcov.info",
    "format": "deno fmt",
    "format:check": "deno fmt --check",
    "lint": "deno lint",
    "lint:fix": "deno lint --fix",
    "documentation": "deno doc ./src/mod.ts"
  },
  "test": {
    "include": ["./tests/**/*.test.mjs"]
  },
  "fmt": {
    "semiColons": false,
    "singleQuote": true
  },
  "imports": {
    "@poolifier/tatami-ng": "jsr:@poolifier/tatami-ng@^0.4.1",
    "@std/assert": "jsr:@std/assert@^0.222.1",
    "@std/cli": "jsr:@std/cli@^0.222.1",
    "@std/testing": "jsr:@std/testing@^0.222.1",
    "benchmark": "npm:benchmark@^2.1.4",
    "expect": "npm:expect@^29.7.0"
  },
  "publish": {
    "include": ["LICENSE", "README.md", "deno.json", "src/**/*.ts"]
  },
  "lock": false,
  "exclude": ["./coverage", "./dist/browser", "./dist/esm", "./npm"]
}
